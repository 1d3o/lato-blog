<%
  row = cell(:elements, :row).new
  block = cell(:elements, :block).new(class: 'xs-12 sm-12 md-12')
%>

<!-- MAIN CONTAINER -->
<%=raw row.open %>

  <%=raw block.open %>

      <%=raw cell(:widgets, :index).new(records: @widget_index_post_fields, index_url: lato_blog.post_fields_path,
      head: [LANGUAGES[:lato_blog][:mixed][:key], LANGUAGES[:lato_blog][:mixed][:typology], LANGUAGES[:lato_blog][:mixed][:visibility]],
      columns: ['key', 'typology', 'meta_visible'],
      actions: {delete: true}, pagination: true) %>

  <%=raw block.close %>

<%=raw row.close %>
<!-- / MAIN CONTAINER -->