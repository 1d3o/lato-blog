<%
  row = cell(:elements, :row).new
  block = cell(:elements, :block).new(class: 'xs-12 sm-12 md-12')
%>

<!-- MAIN CONTAINER -->
<%=raw row.open %>

  <%=raw block.open %>

    <%= render 'lato_blog/back/posts/shared/form', url: lato_blog.posts_path, method: :post %>

  <%=raw block.close %>

<%=raw row.close %>
<!-- / MAIN CONTAINER -->